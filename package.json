  },
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "web": "https://www.bitovi.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitovi/eslint-plugin-nx-glue/issues"
  },
